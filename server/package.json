{
  "name": "server",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.24.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "dev": "pnpm exec nodemon",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 src/openapi/**/* src/mailtrap/template/** dist/",
    "build": "pnpm clean && tsc && pnpm copy-files && tsc-alias",
    "start": "node dist/index.js",
    "seed": "tsx src/scripts/seed.ts",
    "seed:slot": "tsx src/scripts/seed-slot.ts",
    "lint": "eslint . ",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "debug": "nodemon --inspect=9229 -e ts,tsx --exec node -r ts-node/register -r tsconfig-paths/register ./src/index.ts",
    "bundle:swagger": "pnpm exec swagger-cli bundle src/openapi/openapi.yaml -o src/openapi/bundle.yaml --type yaml",
    "preinstall": "pnpm dlx only-allow pnpm"
  },
  "engines": {
    "node": "24.11.0",
    "pnpm": "10.24.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^15.1.3",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/mssql": "^9.1.8",
    "@types/multer": "^2.0.0",
    "@types/nodemailer": "^7.0.4",
    "@types/passport": "^1.0.17",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^11.0.0",
    "bcrypt": "^6.0.0",
    "chalk": "^5.6.2",
    "cloudinary": "^2.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-session": "^1.18.2",
    "handlebars": "^4.7.8",
    "js-yaml": "^4.1.1",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.1",
    "mssql": "^12.2.0",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.11",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "socket.io": "4.8.1",
    "stripe": "20.0.0",
