    "@types/express-session": "^1.18.2",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.10.1",
    "@types/passport-google-oauth20": "^2.0.17",
    "copyfiles": "^2.4.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
